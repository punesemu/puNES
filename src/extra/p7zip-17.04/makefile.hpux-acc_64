
# build 64 bits program (Tested on HPUX with Itanium CPU)

OPTFLAGS=-O

ALLFLAGS=${OPTFLAGS} -mt +DD64  +Z +Ww2101 \
-D_FILE_OFFSET_BITS=64 -D_LARGEFILE_SOURCE \
-DNDEBUG -D_REENTRANT -DENV_UNIX \
$(LOCAL_FLAGS)

CXX=aCC -AA
CC=aCC -Ae

LINK_SHARED=-b
LOCAL_LIBS=
LOCAL_LIBS_DLL=$(LOCAL_LIBS)

OBJ_CRC32=$(OBJ_CRC32_C)
OBJ_AES=

